			<xsd:documentation>
				Половая принадлежность.
			</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="ifx:C">
			<xsd:maxLength value="255"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="HouseExt_Type">
		<xsd:annotation>
			<xsd:documentation>
				Расширение номера дома (номер корпуса, строения).
			</xsd:documentation>
