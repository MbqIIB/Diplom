		<xsd:annotation>
			<xsd:documentation>
				Максимальное количество записей.
			</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="ifx:Long"/>
	</xsd:simpleType>
	<xsd:simpleType name="Memo_Type">
