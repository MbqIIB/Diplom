		<xsd:restriction base="ifx:C">
			<xsd:enumeration value="YES"/>
			<xsd:enumeration value="NO"/>
			<xsd:enumeration value="SEPARATED"/>
			<xsd:enumeration value="RESTRICTED"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="KindPmtCode_Type">
