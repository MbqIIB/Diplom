	<xsd:simpleType name="IdNum_Type">
		<xsd:annotation>
			<xsd:documentation>Номер документа (ДУЛ)</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="ifx:C">
			<xsd:maxLength value="255"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="IdSeries_Type">
		<xsd:annotation>
			<xsd:documentation>
				Серия документа ДУЛ.
			</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="ifx:C">
			<xsd:maxLength value="255"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="IdType_Type">
		<xsd:annotation>
			<xsd:documentation>Тип документа ДУЛ.</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="ifx:C">
			<xsd:maxLength value="255"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="IncAcctBal_Type">
		<xsd:annotation>
			<xsd:documentation>
